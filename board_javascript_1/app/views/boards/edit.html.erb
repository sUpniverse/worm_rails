<form action="/boards/<%@board.id%>/update" method="PUT">
	<input type="hidden" name="authenticity_token" value="<% form_authenticity_token %>">
	제목<br><input type="text" name="title" value="<%=@board.title%>" class="form-control">
	내용<br><input type="text" name="contents" value="<%=@board.contents%>" class="form-control">
	<input type="submit" value="수정하기" class="btn btn-success">
</form>